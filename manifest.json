{
    "name": "[launchpage]",
    "description": "Override the new tab page with a blank one",
    "version": "0.1",
    "incognito": "split",
    "chrome_url_overrides": {
        "newtab": "index.html"
    },
    "background" : {
        "scripts" : ["background.js"]
    },
    "browser_action": {
        "default_title": "[launchpage]",
        "default_icon": "NewIcon128_t.png"
    },
    "icons": {
        "128": "NewIcon128_t.png"
    },
    "manifest_version": 2
}
